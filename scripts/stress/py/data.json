    ],
    "target": "localhost:50057"
  },
  "buildserver": {
    "pkg": "build",
    "svc": "Build",
    "protoDir": "proto/build/build.proto",
    "methods": [
      { "method": "Check", "parameters": {} },
      {
        "method": "ListAbilities",
        "parameters": { "character": { "character_id": 1 } }
      },
      {
        "method": "UpgradeAbility",
        "parameters": {
          "character_id": 1,
          "ability_id": 1,
          "new_ability": false
        }
      }
    ],
    "target": "localhost:50058"
  }
