      { "method": "Check", "parameters": {} },
      { "method": "ListBackpackItems", "parameters": { "character_id": 1 } }
    ],
    "target": "localhost:50053"
  },
  "charserver": {
    "pkg": "char",
    "svc": "Character",
    "protoDir": "proto/char/char.proto",
    "methods": [
      { "method": "Check", "parameters": {} },
      { "method": "ListHeroes", "parameters": {} },
      { "method": "ListCharacters", "parameters": { "username": "oggnjen" } }
    ],
    "target": "localhost:50054"
  },
  "gambaserver": {
    "pkg": "gamba",
    "svc": "Gamba",
