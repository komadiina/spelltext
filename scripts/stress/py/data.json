      { "method": "GetChests", "parameters": {} }
    ],
    "target": "localhost:50055"
  },
  "authserver": {
    "pkg": "auth",
    "svc": "Auth",
    "protoDir": "proto/auth/auth.proto",
    "methods": [
      { "method": "Check", "parameters": {} },
      {
        "method": "Login",
        "parameters": { "username": "oggnjen" }
      }
    ],
    "target": "localhost:50056"
  },
  "combatserver": {
    "pkg": "combat",
    "svc": "Combat",
    "protoDir": "proto/combat/combat.proto",
    "methods": [
      { "method": "Check", "parameters": {} },
