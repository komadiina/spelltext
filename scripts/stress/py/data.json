{
  "chatserver": {
    "pkg": "chat",
    "svc": "Chat",
    "protoDir": "proto/chat/chat.proto",
    "methods": [
      { "method": "Check", "parameters": {} },
      {
        "method": "SendChatMessage",
        "parameters": { "sender": "stress_test", "message": "hello" }
      },
      {
        "method": "JoinChatroom",
        "parameters": { "username": "stress_test_user" }
      },
      {
        "method": "LeaveChatroom",
        "parameters": { "username": "stress_test_user" }
      }
    ],
    "target": "localhost:50051"
  },
  "storeserver": {
    "pkg": "store",
    "svc": "Store",
    "protoDir": "proto/store/store.proto",
    "methods": [
      { "method": "Check", "parameters": {} },
      {
        "method": "ListVendors",
        "parameters": { "noop": "noopnoopnoopnoopnoop" }
      }
    ],
    "target": "localhost:50052"
  },
  "inventoryserver": {
    "pkg": "inventory",
    "svc": "Inventory",
    "protoDir": "proto/inventory/inventory.proto",
